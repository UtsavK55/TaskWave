{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@constants": ["src/constants"],
      "@constants/*": ["src/constants/*"],
      "@contexts/*": ["src/contexts/*"],
      "@helpers": ["src/helpers"],
      "@helpers/*": ["src/helpers/*"],
      "@hooks/*": ["src/hooks/*"],
      "@navigation/*": ["src/navigation/*"],
      "@screens/*": ["src/screens/*"],
      "@network/*": ["src/network/*"],
      "@storage": ["src/storage"],
      "@storage/*": ["src/storage/*"],
      "@theme/*": ["src/theme/*"],
      "@types": ["src/types"],
      "@types/*": ["src/types/*"]
    },
    "types": [],
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "jsx": "react-native",
    "typeRoots": ["src/@types"]
  }
}
